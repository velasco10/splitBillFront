{
  "expo": {
    "name": "billary",
    "slug": "splitBillFront",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/billary-icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/billary-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/billary-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "android.permission.CAMERA"
      ],
      "package": "com.velascode.splitBillFront"
    },
    "web": {
      "favicon": "./assets/billary-icon.png"
    },
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "45e1a50c-012c-4fe9-a1c4-0e2b91bed688"
      }
    },
    "owner": "velascode"
  }
}
